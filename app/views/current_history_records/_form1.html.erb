<table class="table table-condensed">
<tr>
    <td style="width:100px">
    <div class="form-group">
    <%= f.label :recordTime, "就诊时间", style:"font-size:20px", class:"btn-info"%>
    <%= f.date_field :recordTime, class:"form-control"%>
</div> </td>
    <td style="width:100px">
<div class="form-group">
    <%= f.label :docName, "门诊医生", class:"text-success", style:"font-size:20px", class:"btn-info"%>
    <%= f.text_field :docName, value: @current_doctor.name , class:"form-control"%>
</div></td>
