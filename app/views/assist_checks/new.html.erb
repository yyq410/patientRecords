<%= form_for @assistCheck, url: nervous_system_assist_check_path(@nervous_system), :method => :post do |f| %>
    <%= render :partial => "form", :locals => {:f => f} %>
    <%= f.submit "输入辅助检查和拟诊", class:"btn btn-lg btn-success" %>
<% end %>

<% content_for :side_bar do%>
    <%= render "shared/sidebar" %>
<% end %>
<% content_for :side_bar2 do%>
    <%= render :partial => "assistList" %>
<% end %>
